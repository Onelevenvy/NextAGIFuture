{
  "compilerOptions": {
    "target": "ES2016",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/Markdown/Markdown.tsx", "src/components/Markdown/CopyButton.tsx", "src/components/Common/PaneStateControl", "src/components/Icons/modelProvider/index.tsx", "src/assets/icons/models/svg2icon.js", "src/i18n/en-US/chat", "src/i18n/zh-Hans/chat.ts"],
  "exclude": ["node_modules"]
}
